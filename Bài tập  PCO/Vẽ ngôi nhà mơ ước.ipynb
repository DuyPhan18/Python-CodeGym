    "import turtle\n",
    "\n",
    "h= 200\n",
    "r= 180\n",
    "\n",
    "i = 1\n",
    "turtle.goto(100, -100)\n",
    "while i <= 2:\n",
    "    turtle.fd(r)\n",
    "    turtle.rt(90)\n",
    "    turtle.fd(h)\n",
    "    turtle.rt(90)\n",
    "    i -= 1"
